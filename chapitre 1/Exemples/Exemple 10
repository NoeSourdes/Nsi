print("exemple 10")

a,b,c = 4,2,1 # Les variables a, b et c reçoivent les valeurs 4, 2 et 1
print('a est strictement inférieur à b?',a<b) # on vérifie si a est strictement inférieur à b
print('b est strictement supérieur à c?',b>c) # on vérifie si b est strictement supérieur à c
d=a<b # d est une variable binaire qui vérifie si a est strictement inférieur à b
print('a est strictement inférieur à b?',d)
print(type(d))
e=(b>c) # d est une variable binaire qui vérifie si b est strictement supérieur à c
print('b est strictement supérieur à c?',e)
print(type(e))
